<script>

document.getElementById("buttonForm").addEventListener("click", function(e){
    let username = document.getElementById("username").value;
    let password = document.getElementById("password").value;

    myFunctionWEBAPICALL(username, password);
})    
    
async function myFunctionWEBAPICALL(usr, psw){
    
    alert(usr + "  " + psw);
    const requestOption = {
        method: 'POST',
        headers: { 'Content-Type' : 'application/x-www-form-urlencoded' },
        body: "username="+usr+"&password="+psw

    }

    const response = await fetch("http://localhost:3008/login", requestOption);
    const json = await response.json();
    console.log(json.accessToken);
    localStorage.setItem('tokenSession', json.accessToken);
}    


</script>


